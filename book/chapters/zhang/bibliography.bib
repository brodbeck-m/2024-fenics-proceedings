	month = may,
	year = {2015},
	note = {Issue: 6
ISSN: 20412991},
	keywords = {Solid brake disc, heat analysis, porous brake disc, transient, ventilated pin-finned brake disc},
	pages = {674--683},
}

@techreport{voller1_analysis_2003,
	title = {Analysis of automotive disc brake cooling characteristics},
	abstract = {The aim of this investigation was to study automotive disc brake cooling characteristics experimentally using a specially developed spin rig and numerically using gnite element (FE) and computational luid dynamics (CFD) methods. All three modes of heat transfer (conduction, convec-tion and radiation) have been analysed along with the design features of the brake assembly and their interfaces. The spin rig proved to be very valuable equipment; experiments enabled the determination of the thermal contact resistance between the disc and wheel carrier. The analyses demonstrated the sensitivity of this mode of heat transfer to clamping pressure. For convective cooling, heat transfer coecients were measured and very similar results were obtained from spin rig experiments and CFD analyses. The nature of radiative heat dissipation implies substantial eects at high temperatures. The results indicate substantial change of emissivity throughout the brake application. The innuence of brake cooling parameters on the disc temperature has been investigated by FE modelling of a long drag brake application. The thermal power dissipated during the drag brake application has been analysed to reveal the contribution of each mode of heat transfer.},
	author = {Voller1, G P and Tirovic1, M and Morris2, R and Gibbens2, P},
	year = {2003},
	keywords = {brake disc cooling, heat transfer, thermal analysis, thermal contact resistance},
}

