	urldate = {2024-06-26},
	journal = {Measurement},
	author = {Ishak, Mohd Razmi and Abu Bakar, Abd Rahim and Belhocine, Ali and Taib, Jamaludin Mohd and Omar, Wan Zaidi Wan},
