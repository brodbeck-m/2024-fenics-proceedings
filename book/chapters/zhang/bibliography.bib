@article{mackin_thermal_2002,
	title = {Thermal cracking in disc brakes},
	volume = {9},
	issn = {13506307},
	doi = {10.1016/S1350-6307(00)00037-6},
	abstract = {Disc brakes are exposed to large thermal stresses during routine braking and extraordinary thermal stresses during hard braking. High-g decelerations typical of passenger vehicles are known to generate temperatures as high as 900°C in a fraction of a second. These large temperature excursions have two possible outcomes: thermal shock that generates surface cracks; and/or large amounts of plastic deformation in the brake rotor. In the absence of thermal shock, a relatively small number of high-g braking cycles are found to generate macroscopic cracks running through the rotor thickness and along the radius of the disc brake. The analysis herein shows that rotor failure is a consequence of low cycle thermo-mechanical fatigue. An analysis of the vehicle dynamics was used to find a heat flux equation related to braking forces. The heat flux equation was then used in a finite element analysis to determine the temperature profile in the brake. Once the brake temperature was obtained, a simplified shrink fit analysis was used to estimate the stresses that arise during hard braking. This approach shows that plastic deformation occurs due to the large thermal strains associated with high-g braking. The calculated strain amplitude was then used in a Coffin-Manson law to predict the number of high-g braking cycles to failure. Good agreement was obtained between reported braking cycles to failure and the proposed theoretical approach © 2002 Elsevier Science Ltd. All rights reserved.},
	number = {1},
	journal = {Engineering Failure Analysis},
	author = {Mackin, Thomas J. and Noe, Steven C. and Ball, K. J. and Bedell, B. C. and Bim-Merle, D. P. and Bingaman, M. C. and Bomleny, D. M. and Chemlir, G. J. and Clayton, D. B. and Evans, H. A. and Gau, R. and Hart, J. L. and Karney, J. S. and Kiple, B. P. and Kaluga, R. C. and Kung, P. and Law, A. K. and Lim, D. and Merema, R. C. and Miller, B. M. and Miller, T. R. and Nielson, T. J. and O'Shea, T. M. and Olson, M. T. and Padilla, H. A. and Penner, B. W. and Penny, C. and Peterson, R. P. and Polidoro, V. C. and Raghu, A. and Resor, B. R. and Robinson, B. J. and Schambach, D. and Snyder, B. D. and Tom, E. and Tschantz, R. R. and Walker, B. M. and Wasielewski, K. E. and Webb, T. R. and Wise, S. A. and Yang, R. S. and Zimmerman, R. S.},
	year = {2002},
	keywords = {Brake failures, Fatigue, Heat cracks, Thermal fatigue, Thermal stress},
	pages = {63--76},
}

