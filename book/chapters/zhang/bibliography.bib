@article{thuppal_vedanta_heat_2016,
	title = {Heat {Transient} {Transfer} {Analysis} of {Brake} {Disc} /{Pad} {System}},
	abstract = {El frenado es controlado principalmente por el motor. La fricción entre un par de almohadillas y un disco giratorio convierte la energía cinética del vehículo en calor. Se pueden alcanzar altas temperaturas en el sistema, lo que puede ser perjudicial tanto para los componentes como para la seguridad de los pasajeros. Las técnicas numéricas ayudan a simular casos de carga y calculan el campo de temperaturas en el disco de freno y pastillas de freno. El presente trabajo implementa una caja de herramientas de Elemento Finito (FE) en Matlab / Simulink capaz de simular diferentes maniobras de frenado utilizadas para el dimensionamiento de frenos, principalmente en la fase inicial del proceso de desarrollo de automóviles. La geometría de la almohadilla / disco del freno se considera como un cuerpo axisymmetric que asume gradiente de temperatura insignificante a lo largo de la circunferencia del disco. Calibración utilizando tres factores de control a saber: coeficiente de calor durante el frenado, la aceleración y la emisividad para el modelo térmico implementado se realiza mediante una investigación experimental en Volvo Car Corporation (VCC) para tres casos específicos de carga severa. El modelo térmico se extiende para medir la temperatura del líquido de frenos para asegurar que no se produzca vaporización. Los resultados de simulación del disco de freno y de la pastilla de freno muestran una buena correlación con los ensayos experimentales. Un análisis de sensibilidad con los factores de control mostró un coeficiente de convección durante la aceleración el más sensible, con un cambio de temperatura de alrededor del 16\%.},
	author = {Thuppal Vedanta, Srivatsan and Kora, Naga Vamsi Krishna},
	year = {2016},
	keywords = {Brake dimensioning, Brake disc, Calibration, Finite Element, Heat transient analysis},
	pages = {68},
}

