@article{kim_fatigue_2010,
	title = {Fatigue life assessment for brake disc of railway vehicle},
	volume = {33},
	issn = {8756758X},
	doi = {10.1111/j.1460-2695.2009.01412.x},
	abstract = {The brake disc of a railway should have stability. After long-term use, the brake disc may be seriously damaged by thermal fatigue cracks on the frictional surface. In this study, fatigue tests were carried out for a brake disc material (GC25). Furthermore, in order to determine the cause of the thermal fatigue cracks, contact pressure and thermal stress analyses were performed. From these results, the linear relation between temperature and stress variations was obtained, and the remaining life assessment of the brake disc of a railway vehicle was performed by using it. © 2010 Blackwell Publishing Ltd.},
	number = {1},
	journal = {Fatigue and Fracture of Engineering Materials and Structures},
